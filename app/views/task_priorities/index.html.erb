<h1>Listing Task Priorities</h1>

<table class="list">
  <tr>
    <th>Name</th>
    <th>Description</th>
  </tr>

<% @task_priorities.each do |task_priority| %>
  <tr>
    <td><%= task_priority.name %></td>
    <td><%= task_priority.description %></td>
    <td><%= link_to_show task_priority %></td>
    <td><%= link_to_edit edit_task_priority_path(task_priority) %></td>
    <td><%= link_to_destroy task_priority %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to_new 'task priority', new_task_priority_path %>
