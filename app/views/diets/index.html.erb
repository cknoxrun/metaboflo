<h1>Listing diets</h1>

<table class="list">
  <tr>
    <th>Name</th>
		<th colspan="3">Actions</th>
  </tr>

<% @diets.each do |diet| %>
  <tr>
    <td><%= diet.name %></td>
    <td><%= show_link diet %></td>
    <td><%= edit_link edit_diet_path(diet) %></td>
    <td><%= destroy_link diet %></td>
  </tr>
<% end %>
</table>

<br />

<%= new_link 'diet', new_diet_path %>
