<h1>New <%= humanized_type @type %> group</h1>

<%= render_form_errors(@grouping) %>
<%= knoxy_form_for( @grouping, as: :grouping, url: { action: "create", type: @type } ) do |form| %>
  <%= render partial: 'form', locals: { f: form } %>

  <div class="field-no-label actions">
    <%= form.submit "Create" %>
  </div>
<% end %>
