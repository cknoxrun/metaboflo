<h1>All Protocols</h1>

<table class="list">
  <tr>
    <th>Name</th>
    <th>Version</th>
		<th colspan="3">Actions</th>
  </tr>

<% @protocols.each do |protocol| %>
<tr class="<%= cycle('odd', 'even') %>">
    <td><%= protocol.name %></td>
    <td><%= protocol.version %></td>
    <td><%= link_to_show protocol %></td>
    <td><%= link_to_edit edit_protocol_path(protocol) %></td>
    <td><%= link_to_destroy protocol %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to_new 'protocol', new_protocol_path %>
